<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>serverDispatch</servlet-name>
    <servlet-class>com.v2t.server.ServerDispatchImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>serverDispatch</servlet-name>
    <url-pattern>/mvngwt/serverDispatch</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>MVNGwt.html</welcome-file>
  </welcome-file-list>

</web-app>
